<Window x:Class="NumDesTools.UI.InputFormularWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="公式链接修复" Height="400" Width="500">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="31*"/>
            <ColumnDefinition Width="219*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.ColumnSpan="2">
            <ItemsControl Name="ItemsControl" Margin="10"/>
            <Button Content="确认" HorizontalAlignment="Right" Width="100" Margin="10" Click="ConfirmButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
